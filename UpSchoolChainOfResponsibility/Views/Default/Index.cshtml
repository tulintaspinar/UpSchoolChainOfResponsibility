@model WithDrawModel
@{
    Layout = null;
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">

<div class="container">
    <br />
    <form method="post">
        <label>Müşteri Adı</label>
        <input type="text" class="form-control" asp-for="CustomerName" />
        <br />
        <label>Çekilecek Tutar</label>
        <input type="text" class="form-control" asp-for="Amount" />
        <br />
        <button type="submit" class="btn btn-primary">PARA ÇEKME TALEBİNDE BULUN</button>
    </form>
</div>
